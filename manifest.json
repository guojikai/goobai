{
    "manifest_version": 2,
    "name": "Goobai",
    "version": "0.1.0",
    "description": "谷歌百度一键切换",

    "permissions": [
        "storage",
        "*://*.google.com/*",
        "*://*.baidu.com/*"
    ],

    "options_page": "options.html",
    "page_action": {
      "default_icon": "resources/icon.png",
      "default_title": "Goobai"
    },

    "content_scripts": [{
        "matches": ["*://*.google.com/*", "*://*.baidu.com/*"],
        "js": ["resources/lib.js", "resources/app.js"]
    }]
}
